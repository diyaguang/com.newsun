<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.WebOptionEntity" table="WebOption" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="setKey" column="SetKey"/>
        <property name="setName" column="SetName"/>
        <property name="setValue" column="SetValue"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>