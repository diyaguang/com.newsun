<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.WebOptionEntity" table="WebOption" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="setKey">
            <column name="SetKey" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="setName">
            <column name="SetName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="setValue">
            <column name="SetValue" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>