<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.LendDetailEntity" table="LendDetail" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="lendId">
            <column name="LendID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="isLargess">
            <column name="IsLargess" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="productId">
            <column name="ProductID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="money">
            <column name="Money" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="amount">
            <column name="Amount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="realPrice">
            <column name="RealPrice" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="realMoney">
            <column name="RealMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="discount">
            <column name="Discount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="depotCount">
            <column name="DepotCount" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="priceTypeName">
            <column name="PriceTypeName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>