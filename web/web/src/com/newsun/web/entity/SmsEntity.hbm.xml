<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.SmsEntity" table="SMS" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="message" column="Message"/>
        <property name="userId" column="UserID"/>
        <property name="createDate" column="CreateDate"/>
        <property name="errorInfo" column="ErrorInfo"/>
        <property name="opType" column="OpType"/>
        <property name="createType" column="CreateType"/>
        <property name="address" column="Address"/>
        <property name="sendDate" column="SendDate"/>
        <property name="status" column="Status"/>
        <property name="isDel" column="IsDel"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>