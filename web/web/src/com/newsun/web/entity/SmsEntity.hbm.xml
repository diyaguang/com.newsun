<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.SmsEntity" table="SMS" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="message">
            <column name="Message" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="createDate">
            <column name="CreateDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="errorInfo">
            <column name="ErrorInfo" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="opType">
            <column name="OpType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="createType">
            <column name="CreateType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="address">
            <column name="Address" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="sendDate">
            <column name="SendDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="status">
            <column name="Status" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>