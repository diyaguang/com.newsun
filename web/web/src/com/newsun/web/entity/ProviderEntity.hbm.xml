<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ProviderEntity" table="Provider" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="name">
            <column name="Name" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="companyName">
            <column name="CompanyName" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="industry">
            <column name="Industry" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="dutyId">
            <column name="DutyID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="registerAddress">
            <column name="RegisterAddress" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="bank">
            <column name="Bank" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="bankAccount">
            <column name="BankAccount" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="web">
            <column name="Web" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="provideType">
            <column name="ProvideType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="dependencyType">
            <column name="DependencyType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="owenName">
            <column name="OwenName" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="phone">
            <column name="Phone" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="address">
            <column name="Address" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="payMode">
            <column name="PayMode" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="inputUserId">
            <column name="InputUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="date" not-null="true"/>
        </property>
        <property name="modifyUserId">
            <column name="ModifyUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="ModifyDate" sql-type="date" not-null="true"/>
        </property>
        <property name="state">
            <column name="State" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>