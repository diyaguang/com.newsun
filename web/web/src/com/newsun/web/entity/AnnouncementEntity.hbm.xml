<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.AnnouncementEntity" table="Announcement" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="announcementType" column="AnnouncementType"/>
        <property name="title" column="Title"/>
        <property name="contents" column="Contents"/>
        <property name="announcementDate" column="AnnouncementDate"/>
        <property name="note" column="Note"/>
        <property name="approveState" column="ApproveState"/>
        <property name="inputUserId" column="InputUserID"/>
        <property name="inputDate" column="InputDate"/>
        <property name="modifyUserId" column="ModifyUserID"/>
        <property name="modifyDate" column="ModifyDate"/>
        <property name="state" column="State"/>
        <property name="isDel" column="IsDel"/>
        <property name="editId" column="EditID"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>