<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ExpressStateEntity" table="ExpressState" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="consignmentId" column="ConsignmentID"/>
        <property name="stateType" column="StateType"/>
        <property name="opDate" column="OpDate"/>
        <property name="userId" column="UserID"/>
        <property name="isDel" column="IsDel"/>
        <property name="note" column="Note"/>
        <property name="editId" column="EditID"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>