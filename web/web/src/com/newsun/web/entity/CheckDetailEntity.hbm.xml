<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.CheckDetailEntity" table="CheckDetail" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="checkId">
            <column name="CheckID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="productId">
            <column name="ProductID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="checkCount">
            <column name="CheckCount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="accountCount">
            <column name="AccountCount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="checkMoney">
            <column name="CheckMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="amount">
            <column name="Amount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>