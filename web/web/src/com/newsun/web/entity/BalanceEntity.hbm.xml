<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.BalanceEntity" table="Balance" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="clientId">
            <column name="ClientID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="opUserName">
            <column name="OpUserName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="date">
            <column name="Date" sql-type="datetime" not-null="true"/>
        </property>
        <property name="billCode">
            <column name="BillCode" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="realMoney">
            <column name="RealMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="beforehandMoney">
            <column name="BeforehandMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="useBeforehand">
            <column name="UseBeforehand" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputUserId">
            <column name="InputUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="date" not-null="true"/>
        </property>
        <property name="modifyUserId">
            <column name="ModifyUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="ModifyDate" sql-type="date" not-null="true"/>
        </property>
        <property name="approveState">
            <column name="ApproveState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="editId">
            <column name="EditID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="isUsed">
            <column name="IsUsed" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>