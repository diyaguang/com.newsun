<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.EmailUserLinkEntity" table="Email_User_Link" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="emailId">
            <column name="EmailID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>