<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ExpressStateLogEntity" table="ExpressStateLog" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="expressStateId">
            <column name="ExpressStateID" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="opDate">
            <column name="OpDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="stateType">
            <column name="StateType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>