<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ExpressEntity" table="Express" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="name" column="Name"/>
        <property name="phone" column="Phone"/>
        <property name="userName" column="UserName"/>
        <property name="userPhone" column="UserPhone"/>
        <property name="takeType" column="TakeType"/>
        <property name="takePlace" column="TakePlace"/>
        <property name="inputUserId" column="InputUserID"/>
        <property name="inputDate" column="InputDate"/>
        <property name="modifyUserId" column="ModifyUserID"/>
        <property name="modifyDate" column="ModifyDate"/>
        <property name="isDel" column="IsDel"/>
        <property name="note" column="Note"/>
        <property name="systemPath" column="SystemPath"/>
        <property name="queryUrl" column="QueryUrl"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>