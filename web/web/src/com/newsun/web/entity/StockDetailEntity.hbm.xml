<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.StockDetailEntity" table="StockDetail" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="stockId">
            <column name="StockID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="productId">
            <column name="ProductID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="amount">
            <column name="Amount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="money">
            <column name="Money" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="alreadyArrive">
            <column name="AlreadyArrive" sql-type="decimal(10)" precision="10" not-null="true"/>
        </property>
        <property name="noDutyMoney">
            <column name="NoDutyMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="noDutyPrice">
            <column name="NoDutyPrice" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="duty">
            <column name="Duty" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="stockType">
            <column name="StockType" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="stockTax">
            <column name="StockTax" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>