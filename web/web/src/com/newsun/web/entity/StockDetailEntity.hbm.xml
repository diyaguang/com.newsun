<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.StockDetailEntity" table="StockDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="stockId" column="StockID"/>
        <property name="productId" column="ProductID"/>
        <property name="amount" column="Amount"/>
        <property name="price" column="Price"/>
        <property name="money" column="Money"/>
        <property name="note" column="Note"/>
        <property name="alreadyArrive" column="AlreadyArrive"/>
        <property name="noDutyMoney" column="NoDutyMoney"/>
        <property name="noDutyPrice" column="NoDutyPrice"/>
        <property name="duty" column="Duty"/>
        <property name="stockType" column="StockType"/>
        <property name="stockTax" column="StockTax"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>