<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.LendLogEntity" table="LendLog" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="lendId">
            <column name="LendID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="changeSaleAmount">
            <column name="ChangeSaleAmount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="changeSaleMoney">
            <column name="ChangeSaleMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="amount">
            <column name="Amount" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="money">
            <column name="Money" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="opDate">
            <column name="OpDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="opUser">
            <column name="OpUser" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="productId">
            <column name="ProductID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>