<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.LendLogEntity" table="LendLog" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="lendId" column="LendID"/>
        <property name="changeSaleAmount" column="ChangeSaleAmount"/>
        <property name="changeSaleMoney" column="ChangeSaleMoney"/>
        <property name="amount" column="Amount"/>
        <property name="money" column="Money"/>
        <property name="opDate" column="OpDate"/>
        <property name="opUser" column="OpUser"/>
        <property name="productId" column="ProductID"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>