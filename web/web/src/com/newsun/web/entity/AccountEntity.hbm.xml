<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.AccountEntity" table="Account" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="type">
            <column name="Type" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="name">
            <column name="Name" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="inputUserId">
            <column name="InputUserID" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="date" not-null="true"/>
        </property>
        <property name="modifyUserId">
            <column name="ModifyUserID" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="ModifyDate" sql-type="date" not-null="true"/>
        </property>
        <property name="approveState">
            <column name="ApproveState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>