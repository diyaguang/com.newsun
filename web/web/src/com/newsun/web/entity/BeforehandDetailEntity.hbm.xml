<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.BeforehandDetailEntity" table="BeforehandDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="beforehandId" column="BeforehandID"/>
        <property name="accountId" column="AccountID"/>
        <property name="money" column="Money"/>
        <property name="billCode" column="BillCode"/>
        <property name="note" column="Note"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>