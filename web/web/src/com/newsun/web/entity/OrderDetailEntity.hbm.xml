<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.OrderDetailEntity" table="OrderDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="orderId" column="OrderID"/>
        <property name="productId" column="ProductID"/>
        <property name="amount" column="Amount"/>
        <property name="price" column="Price"/>
        <property name="money" column="Money"/>
        <property name="isLargess" column="IsLargess"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>