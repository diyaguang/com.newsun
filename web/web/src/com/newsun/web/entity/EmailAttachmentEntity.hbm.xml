<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.EmailAttachmentEntity" table="EmailAttachment" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="emailId">
            <column name="EmailID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="name">
            <column name="Name" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="path">
            <column name="Path" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="upLoadDate">
            <column name="UpLoadDate" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>