<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.EmailAttachmentEntity" table="EmailAttachment" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="emailId" column="EmailID"/>
        <property name="name" column="Name"/>
        <property name="path" column="Path"/>
        <property name="upLoadDate" column="UpLoadDate"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>