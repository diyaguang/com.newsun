<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ClientAccountEntity" table="ClientAccount" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="int(11)"/>
        </id>
        <property name="opType">
            <column name="OpType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="opCode">
            <column name="OpCode" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="opId">
            <column name="OpID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="money">
            <column name="Money" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="date">
            <column name="Date" sql-type="date" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="clientId">
            <column name="ClientID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>