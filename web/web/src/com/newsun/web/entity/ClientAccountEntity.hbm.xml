<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ClientAccountEntity" table="ClientAccount" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="opType" column="OpType"/>
        <property name="opCode" column="OpCode"/>
        <property name="opId" column="OpID"/>
        <property name="money" column="Money"/>
        <property name="date" column="Date"/>
        <property name="userId" column="UserID"/>
        <property name="clientId" column="ClientID"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>