<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.FunctionsEntity" table="Functions" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="parentId" column="ParentID"/>
        <property name="funcName" column="FuncName"/>
        <property name="funcCode" column="FuncCode"/>
        <property name="state" column="State"/>
        <property name="level" column="Level"/>
        <property name="urlPath" column="UrlPath"/>
        <property name="sort" column="Sort"/>
        <property name="imagePath" column="ImagePath"/>
        <property name="iconName" column="IconName"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>