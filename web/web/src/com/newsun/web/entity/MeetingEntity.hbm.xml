<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.MeetingEntity" table="Meeting" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="name" column="Name"/>
        <property name="meetingRoomId" column="MeetingRoomID"/>
        <property name="startDate" column="StartDate"/>
        <property name="endDate" column="EndDate"/>
        <property name="applyUserId" column="ApplyUserID"/>
        <property name="applyDate" column="ApplyDate"/>
        <property name="approveState" column="ApproveState"/>
        <property name="approveUserId" column="ApproveUserID"/>
        <property name="approveDate" column="ApproveDate"/>
        <property name="editId" column="EditID"/>
        <property name="isDel" column="IsDel"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>