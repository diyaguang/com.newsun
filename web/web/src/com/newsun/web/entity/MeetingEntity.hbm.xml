<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.MeetingEntity" table="Meeting" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="name">
            <column name="Name" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="meetingRoomId">
            <column name="MeetingRoomID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="startDate">
            <column name="StartDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="endDate">
            <column name="EndDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="applyUserId">
            <column name="ApplyUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="applyDate">
            <column name="ApplyDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="approveState">
            <column name="ApproveState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="approveUserId">
            <column name="ApproveUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="approveDate">
            <column name="ApproveDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="editId">
            <column name="EditID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>