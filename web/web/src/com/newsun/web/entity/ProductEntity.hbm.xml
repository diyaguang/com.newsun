<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ProductEntity" table="Product" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="code">
            <column name="Code" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="model">
            <column name="Model" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="area">
            <column name="Area" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="barCode">
            <column name="BarCode" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="duty">
            <column name="Duty" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="masterUnit">
            <column name="MasterUnit" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="assistUnit">
            <column name="AssistUnit" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="unitChange">
            <column name="UnitChange" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="type">
            <column name="Type" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="providerId">
            <column name="ProviderID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="depositPlace">
            <column name="DepositPlace" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="inputUserId">
            <column name="InputUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="date" not-null="true"/>
        </property>
        <property name="modifyUserId">
            <column name="ModifyUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="ModifyDate" sql-type="date" not-null="true"/>
        </property>
        <property name="state">
            <column name="State" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="imagePath">
            <column name="ImagePath" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="shortName">
            <column name="ShortName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="categoryOne">
            <column name="CategoryOne" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="categoryTwo">
            <column name="CategoryTwo" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="depotId">
            <column name="DepotID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>