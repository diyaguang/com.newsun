<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ProductEntity" table="Product" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="name" column="Name"/>
        <property name="code" column="Code"/>
        <property name="model" column="Model"/>
        <property name="area" column="Area"/>
        <property name="barCode" column="BarCode"/>
        <property name="duty" column="Duty"/>
        <property name="masterUnit" column="MasterUnit"/>
        <property name="assistUnit" column="AssistUnit"/>
        <property name="unitChange" column="UnitChange"/>
        <property name="type" column="Type"/>
        <property name="providerId" column="ProviderID"/>
        <property name="depositPlace" column="DepositPlace"/>
        <property name="note" column="Note"/>
        <property name="inputUserId" column="InputUserID"/>
        <property name="inputDate" column="InputDate"/>
        <property name="modifyUserId" column="ModifyUserID"/>
        <property name="modifyDate" column="ModifyDate"/>
        <property name="state" column="State"/>
        <property name="isDel" column="IsDel"/>
        <property name="imagePath" column="ImagePath"/>
        <property name="shortName" column="ShortName"/>
        <property name="categoryOne" column="CategoryOne"/>
        <property name="categoryTwo" column="CategoryTwo"/>
        <property name="depotId" column="DepotID"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>