<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.CommissionEntity" table="Commission" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="billId">
            <column name="BillID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="billCode">
            <column name="BillCode" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="url">
            <column name="Url" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="isRead">
            <column name="IsRead" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="permissionsCode">
            <column name="PermissionsCode" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="createUserId">
            <column name="CreateUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="createDate">
            <column name="CreateDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="readUserId">
            <column name="ReadUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="readDate">
            <column name="ReadDate" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>