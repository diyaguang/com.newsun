<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.CommissionEntity" table="Commission" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="name" column="Name"/>
        <property name="billId" column="BillID"/>
        <property name="billCode" column="BillCode"/>
        <property name="url" column="Url"/>
        <property name="isRead" column="IsRead"/>
        <property name="permissionsCode" column="PermissionsCode"/>
        <property name="createUserId" column="CreateUserID"/>
        <property name="createDate" column="CreateDate"/>
        <property name="readUserId" column="ReadUserID"/>
        <property name="readDate" column="ReadDate"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>