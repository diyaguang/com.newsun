<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.SaleReturnDetailEntity" table="SaleReturnDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="saleReturnId" column="SaleReturnID"/>
        <property name="productId" column="ProductID"/>
        <property name="amount" column="Amount"/>
        <property name="money" column="Money"/>
        <property name="billCode" column="BillCode"/>
        <property name="priceTypeName" column="PriceTypeName"/>
        <property name="price" column="Price"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>