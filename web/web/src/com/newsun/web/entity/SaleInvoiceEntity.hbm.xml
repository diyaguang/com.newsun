<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.SaleInvoiceEntity" table="SaleInvoice" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="clientId">
            <column name="ClientID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="opUserName">
            <column name="OpUserName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="opDate">
            <column name="OpDate" sql-type="date" not-null="true"/>
        </property>
        <property name="invoiceType">
            <column name="InvoiceType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="noDutyMoney">
            <column name="NoDutyMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="duty">
            <column name="Duty" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="money">
            <column name="Money" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="balanceState">
            <column name="BalanceState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputUserId">
            <column name="InputUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="date" not-null="true"/>
        </property>
        <property name="modifyUserId">
            <column name="ModifyUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="ModifyDate" sql-type="date" not-null="true"/>
        </property>
        <property name="approveState">
            <column name="ApproveState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="editId">
            <column name="EditID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="billCode">
            <column name="BillCode" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>