<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.TaskOpDetailEntity" table="TaskOpDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="taskId" column="TaskID"/>
        <property name="userId" column="UserID"/>
        <property name="opDate" column="OpDate"/>
        <property name="opType" column="OpType"/>
        <property name="contents" column="Contents"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>