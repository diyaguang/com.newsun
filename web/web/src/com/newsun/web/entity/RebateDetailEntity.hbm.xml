<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.RebateDetailEntity" table="RebateDetail" schema="dygstudio">
        <property name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="rebateId">
            <column name="RebateID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="balanceId">
            <column name="BalanceID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>