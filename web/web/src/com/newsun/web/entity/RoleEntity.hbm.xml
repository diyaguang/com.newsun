<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.RoleEntity" table="Role" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="roleName">
            <column name="RoleName" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="state">
            <column name="State" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>