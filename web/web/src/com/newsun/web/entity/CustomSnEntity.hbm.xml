<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.CustomSnEntity" table="CustomSN" schema="dygstudio">
        <property name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="number">
            <column name="Number" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="date">
            <column name="Date" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="type">
            <column name="Type" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>