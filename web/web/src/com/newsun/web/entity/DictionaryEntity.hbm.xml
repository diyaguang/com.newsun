<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.DictionaryEntity" table="Dictionary" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="type">
            <column name="Type" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="code">
            <column name="Code" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="name">
            <column name="Name" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="state">
            <column name="State" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>