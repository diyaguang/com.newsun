<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.BomEntity" table="Bom" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="name" column="Name"/>
        <property name="productId" column="ProductID"/>
        <property name="amount" column="Amount"/>
        <property name="note" column="Note"/>
        <property name="state" column="State"/>
        <property name="inputUserId" column="InputUserID"/>
        <property name="inputDate" column="InputDate"/>
        <property name="modfiyUserId" column="ModfiyUserID"/>
        <property name="modifyDate" column="ModifyDate"/>
        <property name="isDel" column="IsDel"/>
        <property name="price" column="Price"/>
        <property name="editId" column="EditID"/>
        <property name="approveState" column="ApproveState"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>