<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.RebateEntity" table="Rebate" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="rebateType">
            <column name="RebateType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="rebateDate">
            <column name="RebateDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="money">
            <column name="Money" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="discount">
            <column name="Discount" sql-type="decimal(18,4)" precision="18" scale="4" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="editId">
            <column name="EditID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputUserId">
            <column name="InputUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="modifyUserId">
            <column name="ModifyUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="ModifyDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="approveState">
            <column name="ApproveState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="clientId">
            <column name="ClientID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="balanceMoney">
            <column name="BalanceMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>