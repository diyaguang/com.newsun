<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.AllotEntity" table="Allot" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="inDepotId">
            <column name="InDepotID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="outDepotId">
            <column name="OutDepotID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="carId">
            <column name="CarID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="address">
            <column name="Address" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="isUseCar">
            <column name="IsUseCar" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="outDate">
            <column name="OutDate" sql-type="date" not-null="true"/>
        </property>
        <property name="inDate">
            <column name="InDate" sql-type="date" not-null="true"/>
        </property>
        <property name="note">
            <column name="Note" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="state">
            <column name="State" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="approveState">
            <column name="ApproveState" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="inputUserId">
            <column name="InputUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="date" not-null="true"/>
        </property>
        <property name="modifyUserId">
            <column name="ModifyUserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="modifyDate">
            <column name="ModifyDate" sql-type="date" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="editId">
            <column name="EditID" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>