<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.AllotEntity" table="Allot" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="inDepotId" column="InDepotID"/>
        <property name="outDepotId" column="OutDepotID"/>
        <property name="userId" column="UserID"/>
        <property name="carId" column="CarID"/>
        <property name="address" column="Address"/>
        <property name="isUseCar" column="IsUseCar"/>
        <property name="outDate" column="OutDate"/>
        <property name="inDate" column="InDate"/>
        <property name="note" column="Note"/>
        <property name="state" column="State"/>
        <property name="approveState" column="ApproveState"/>
        <property name="inputUserId" column="InputUserID"/>
        <property name="inputDate" column="InputDate"/>
        <property name="modifyUserId" column="ModifyUserID"/>
        <property name="modifyDate" column="ModifyDate"/>
        <property name="isDel" column="IsDel"/>
        <property name="editId" column="EditID"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>