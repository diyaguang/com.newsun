<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.StorageAccountTotalEntity" table="StorageAccountTotal" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="depotId" column="DepotID"/>
        <property name="productId" column="ProductID"/>
        <property name="userId" column="UserID"/>
        <property name="opDate" column="OpDate"/>
        <property name="amount" column="Amount"/>
        <property name="money" column="Money"/>
        <property name="isUsed" column="IsUsed"/>
        <property name="isDel" column="IsDel"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>