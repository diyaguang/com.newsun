<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.PriceEntity" table="Price" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="priceType" column="PriceType"/>
        <property name="productId" column="ProductID"/>
        <property name="model" column="Model"/>
        <property name="price" column="Price"/>
        <property name="modifyPrice" column="ModifyPrice"/>
        <property name="masterUnit" column="MasterUnit"/>
        <property name="lastModifyDate" column="LastModifyDate"/>
        <property name="approveUserId" column="ApproveUserID"/>
        <property name="approveDate" column="ApproveDate"/>
        <property name="note" column="Note"/>
        <property name="inputUserId" column="InputUserID"/>
        <property name="inputDate" column="InputDate"/>
        <property name="modifyUserId" column="ModifyUserID"/>
        <property name="modifyDate" column="ModifyDate"/>
        <property name="state" column="State"/>
        <property name="isDel" column="IsDel"/>
        <property name="approveState" column="ApproveState"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>