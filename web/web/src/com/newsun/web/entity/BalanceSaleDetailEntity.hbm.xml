<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.BalanceSaleDetailEntity" table="BalanceSaleDetail" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="balanceId">
            <column name="BalanceID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="accountBillId">
            <column name="AccountBillID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="balanceMoney">
            <column name="BalanceMoney" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>