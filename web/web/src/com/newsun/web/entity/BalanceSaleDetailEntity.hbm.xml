<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.BalanceSaleDetailEntity" table="BalanceSaleDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="balanceId" column="BalanceID"/>
        <property name="accountBillId" column="AccountBillID"/>
        <property name="balanceMoney" column="BalanceMoney"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>