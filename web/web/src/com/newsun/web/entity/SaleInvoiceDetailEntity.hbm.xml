<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.SaleInvoiceDetailEntity" table="SaleInvoiceDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="saleInvoiceId" column="SaleInvoiceID"/>
        <property name="saleId" column="SaleID"/>
        <property name="billType" column="BillType"/>
        <property name="billDate" column="BillDate"/>
        <property name="money" column="Money"/>
        <property name="billCode" column="BillCode"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>