<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.SaleInvoiceDetailEntity" table="SaleInvoiceDetail" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="saleInvoiceId">
            <column name="SaleInvoiceID" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="saleId">
            <column name="SaleID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="billType">
            <column name="BillType" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="billDate">
            <column name="BillDate" sql-type="date" not-null="true"/>
        </property>
        <property name="money">
            <column name="Money" sql-type="decimal(18,2)" precision="18" scale="2" not-null="true"/>
        </property>
        <property name="billCode">
            <column name="BillCode" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>