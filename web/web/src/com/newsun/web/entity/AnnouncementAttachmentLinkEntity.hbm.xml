<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.AnnouncementAttachmentLinkEntity" table="Announcement_Attachment_link"
           schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="announcementId">
            <column name="AnnouncementID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="attachmentId">
            <column name="AttachmentID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>