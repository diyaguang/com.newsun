<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.SaleDetailEntity" table="SaleDetail" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="saleId" column="SaleID"/>
        <property name="isLargess" column="IsLargess"/>
        <property name="productId" column="ProductID"/>
        <property name="price" column="Price"/>
        <property name="money" column="Money"/>
        <property name="amount" column="Amount"/>
        <property name="realPrice" column="RealPrice"/>
        <property name="realMoney" column="RealMoney"/>
        <property name="note" column="Note"/>
        <property name="discount" column="Discount"/>
        <property name="depotCount" column="DepotCount"/>
        <property name="priceTypeName" column="PriceTypeName"/>
        <property name="bzPrice" column="BzPrice"/>
        <property name="bzDiscount" column="BzDiscount"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>