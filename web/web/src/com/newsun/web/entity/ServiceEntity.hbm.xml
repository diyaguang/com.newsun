<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ServiceEntity" table="Service" schema="dygstudio">
        <id name="id" column="ID"/>
        <property name="code" column="Code"/>
        <property name="clientId" column="ClientID"/>
        <property name="title" column="Title"/>
        <property name="contents" column="Contents"/>
        <property name="inputDate" column="InputDate"/>
        <property name="isRevert" column="IsRevert"/>
        <property name="revertContents" column="RevertContents"/>
        <property name="revertDate" column="RevertDate"/>
        <property name="userId" column="UserID"/>
        <property name="isDel" column="IsDel"/>
        <property name="bookId" column="BookID"/>
        <property name="companyId" column="CompanyID"/>
    </class>
</hibernate-mapping>