<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newsun.web.entity.ServiceEntity" table="Service" schema="dygstudio">
        <id name="id">
            <column name="ID" sql-type="varchar(40)" length="40"/>
        </id>
        <property name="code">
            <column name="Code" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="clientId">
            <column name="ClientID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="title">
            <column name="Title" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="contents">
            <column name="Contents" sql-type="text" not-null="true"/>
        </property>
        <property name="inputDate">
            <column name="InputDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="isRevert">
            <column name="IsRevert" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="revertContents">
            <column name="RevertContents" sql-type="text" not-null="true"/>
        </property>
        <property name="revertDate">
            <column name="RevertDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="userId">
            <column name="UserID" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <property name="isDel">
            <column name="IsDel" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>